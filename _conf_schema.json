{
    "base_url": {
        "description": "API Base URL",
        "type": "string",
        "default": "https://ai.gitee.com/v1",
        "hint": "Gitee AI API 地址"
    },
    "api_key": {
        "description": "API 密钥 (支持多 Key 轮询)",
        "type": "list",
        "hint": "Gitee AI API Key，支持配置多个 Key 以实现轮询调用",
        "default": [],
        "obvious_hint": true
    },
    "model": {
        "description": "使用的模型名称",
        "type": "string",
        "default": "z-image-turbo",
        "hint": "例如: z-image-turbo"
    },
    "size": {
        "description": "图片大小",
        "type": "string",
        "default": "1024x1024",
        "hint": "例如: 1024x1024"
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "int",
        "default": 9,
        "hint": "推理步数，默认 9"
    },
    "negative_prompt": {
        "description": "负面提示词",
        "type": "string",
        "default": "",
        "hint": "用于指定不希望出现在生成图片中的内容"
    },
    "edit_base_url": {
        "description": "图生图 API Base URL",
        "type": "string",
        "default": "https://ai.gitee.com/v1",
        "hint": "图生图 API 地址，留空则使用文生图的 base_url"
    },
    "edit_api_key": {
        "description": "图生图 API 密钥 (支持多 Key 轮询)",
        "type": "list",
        "hint": "图生图 API Key，留空则使用文生图的 api_key",
        "default": []
    },
    "edit_model": {
        "description": "图生图模型名称",
        "type": "string",
        "default": "Qwen-Image-Edit-2511",
        "hint": "例如: Qwen-Image-Edit-2511"
    },
    "edit_num_inference_steps": {
        "description": "图生图推理步数",
        "type": "int",
        "default": 4,
        "hint": "图生图推理步数，默认 4"
    },
    "edit_guidance_scale": {
        "description": "图生图引导系数",
        "type": "float",
        "default": 1.0,
        "hint": "引导系数，控制生成图片与提示词的一致性，默认 1.0"
    },
    "edit_poll_interval": {
        "description": "图生图轮询间隔(秒)",
        "type": "int",
        "default": 5,
        "hint": "轮询任务状态的间隔时间，默认 5 秒"
    },
    "edit_poll_timeout": {
        "description": "图生图轮询超时(秒)",
        "type": "int",
        "default": 300,
        "hint": "轮询超时时间，默认 300 秒(5分钟)"
    }
}